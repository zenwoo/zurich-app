{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "next/core-web-vitals"
  ],
  "rules": {
    "comma-dangle": ["error", "never"]
  },
  "overrides": [{
    "files": ["**/*Slice.js"],
    "rules": {
      "no-param-reassign": ["error", { "props": false }]
    }
  }]
}
